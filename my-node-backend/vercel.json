{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/App.tsx",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "headers":{
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  
