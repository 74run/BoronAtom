{
    "version": 2,
    "builds": [
      {
        "src": "my-node-backend/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "headers":{
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  
